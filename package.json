  "scripts": {
    "_postinstall": "node -e \"if(require('fs').existsSync('.git')){process.exit(1)}\" || is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "NODE_PATH=\"${PWD}/node_modules\" serverless offline --host 0.0.0.0",
    "lint": "eslint --max-warnings=0",
    "release": "RELEASE=1 standard-version",
    "test:unit": "yarn jest --setupFiles ./.jest/config --watch -c .jest/unit-config.js --colors --no-coverage",
    "test:integration": "yarn jest --setupFiles ./.jest/config --watch -c .jest/integration-config.js --colors --runInBand --no-coverage",
    "test:staged": "yarn jest --setupFiles ./.jest/config -c ./.jest/config.js --colors --findRelatedTests --runInBand",
    "test:ci": "yarn jest --setupFiles ./.jest/config -c ./.jest/ci-config.js --colors --coverage --bail --runInBand --no-cache",
    "start": "ts-node src/main/server.ts"
  }
